# Strict npm configuration
registry=https://registry.npmjs.org/
strict-ssl=true
save-exact=true
engines-strict=true
engine-strict=true
loglevel=error
progress=false
fund=false

# Timeout and retry settings
timeout=60000
fetch-retries=3
fetch-retry-factor=1.5
fetch-retry-maxtimeout=60000

# Security settings
audit=true
audit-level=high
